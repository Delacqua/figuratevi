    <!--The div element for the map -->
    <div id="map" class="maps"></div>
    <script>
// Initialize and add the map
function initMap() {
  // The location
  var percorso= {lat: 43.111393, lng: 12.355068};
  // The map centered
  var map = new google.maps.Map(
      document.getElementById('map'), {zoom: 14, center: percorso});
  // The marker, positioned at percorso
  var marker = new google.maps.Marker({position: percorso, map: map});
}
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC7gKEou3qs-fIbVwW7lqeqUez9AUiOV10&callback=initMap">
    </script>